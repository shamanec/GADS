            "post": {
                "description": "Starts building a docker image in a goroutine and just returns Accepted.",
                "tags": [
                    "configuration"
                ],
                "summary": "Build docker images",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Image type: ios-appium, android-appium",
                        "name": "image_type",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "202": {
                        "description": ""
                    }
                }
            }
        },
